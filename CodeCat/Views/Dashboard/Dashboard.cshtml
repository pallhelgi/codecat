@model CodeCat.Models.ViewModels.DashboardViewModel

@{
    ViewBag.Title = "Dashboard";
}

<h2>Dashboard</h2>

<!--Create new project button-->
<section class="col-sm-4">
    <section class="well">
        <h3>@Html.ActionLink("Create a new project", "AddProject", "Dashboard")</h3>
    </section>
</section>

<!--List of projects-->
@foreach (var itemj in Model.projects)
{
    <section class="col-sm-4">
        <section class="well">
            <h3>@Html.DisplayFor(modelItem => itemj.name)</h3>
            <p>ID: @Html.DisplayFor(modelItem => itemj.ID)</p>
            <p>creatorUserID: @Html.DisplayFor(modelItem => itemj.creatorUserID)</p>
        </section>
    </section>
}